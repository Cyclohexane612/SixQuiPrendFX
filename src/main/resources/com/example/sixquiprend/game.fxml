<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>



<StackPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sixquiprend.HelloController">
    <children>
        <ImageView>
            <image>
                <Image url="@../../../image/background.jpeg" />
            </image>
        </ImageView>

        <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="100.0" StackPane.alignment="CENTER_LEFT">

            <VBox alignment="TOP_LEFT" spacing="20.0">
                <children>
                    <ImageView fitWidth="50.0" fitHeight="50.0">
                        <image>
                            <Image url="@../../../image/LOGO.png" />
                        </image>
                    </ImageView>
                    <Button fx:id="play_button" onAction="#onStartButtonClick" text="Play" />
                </children>
                <padding>
                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                </padding>
            </VBox>
        </VBox>
    </children>
</StackPane>
